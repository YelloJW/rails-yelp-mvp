<div class="container card-local shadow-sm mt-3 w-50">
  <h1>New restaurant</h1>
  <hr>
  <%= simple_form_for @restaurant do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :category %>
    <%= f.input :phone_number %>
    <%= f.button :submit, class:"btn btn-primary"%>
  <% end %>
</div>
